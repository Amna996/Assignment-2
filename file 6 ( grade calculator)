#include<stdio.h>
#include<conio.h>
int main(){
	float mark1,mark2,mark3,averge;
	printf("Enter the mark for subject 1");
	scanf("%f",&mark1);
	printf("Enter the mark for subject 2");
	scanf("%f",&mark2);
	printf("Enter the mark for subject 3");
	scanf("%f",&mark3);
	if(averge>=90){
	printf(" grade A");
}	
	else if( averge>=80){
	printf("grade B");
}	
	else if(averge>=60){
	printf(" grade D");
}	
	else {
	printf("fail grade F");
}	
	return 0;
}