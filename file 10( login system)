#include<stdio.h>
#include<conio.h>
int main(){
	int user_name,pass_word;
	printf("Enter your user_name 'admin':");
	scanf("%d",&user_name);
	printf("Enter your pass_word '1234':");
	scanf("%d",&pass_word);
	if (user_name== 'admin' || pass_word =='1234')
	{
	printf("login Succesfull");
	}
	
	else
	{
	printf("login failed");
	}
	return 0;
}